                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath be.fedict.lodtools.sbmb.Main -g -s 1962 -e 1962  -b http://www.ejustice.just.fgov.be/eli -t DECISION -n BESLUIT -f ARRETE -o c:\data\sbmb\decision -w 10</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir>c:\data</exec.workingdir>
            </properties>
        </action>
        <action>
